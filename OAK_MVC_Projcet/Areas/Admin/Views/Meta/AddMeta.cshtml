
@{
    ViewBag.Title = "AddMeta";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";


}

<section class="content-header">
    <h1>Add Meta Operation</h1>
    <ol class="breadcrumb float-sm-left">
        <li class="breadcrumb-item"><a>Admin</a></li>
        <li class="breadcrumb-item"><a>Meta</a></li>
        <li class="breadcrumb-item"><a>Add Meta</a></li>
    </ol>
</section>
<br/>
<div class="card-footer">
    <a type="submit" class="btn btn-primary" href="/Admin/Meta/MetaList">Meta List</a>
</div>

<section class="content">
    <div class="row">
        <div class="col-md-9">
            @Html.Partial("~/Areas/Admin/Views/Shared/_MessageForm.cshtml")
            <div class="box box-primary">
                @using (Html.BeginForm("AddMeta", "Meta", FormMethod.Post, new {@enctype="multipart/form-data"}))
            {
                @Html.Partial("PartialView/_MetaForm")
            }
            </div>
        </div>
    </div>
</section>