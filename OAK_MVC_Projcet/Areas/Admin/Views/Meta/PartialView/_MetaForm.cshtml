@model Data_Transfer_Layer.MetaDataTransfer
    <div class="box-body">
        <div class="form-group">
            <label>Meta Content</label>
            @Html.TextBoxFor(x=>x.MetaContent,new { @class = "form-control",@placeholder="Meta Content" })
            @Html.ValidationMessageFor(x => x.MetaContent, null, new {@class="text-danger"})
        </div>
        <div class="form-group">
            <label>Name</label>
            @Html.DropDownList("Name",new SelectList(new[] { "Keyword", "Description", "Author", "Twittercard" }), new {@class="form-control select2"})
        </div>
        <input type="hidden" name="MetaID" value="@Model.MetaID" />
        <div class="card-footer">
            <button type="submit" class="btn btn-primary">Save</button>
        </div>
    </div>
